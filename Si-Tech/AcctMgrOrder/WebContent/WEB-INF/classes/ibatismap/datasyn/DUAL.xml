<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="BK_dual">
  
  <select id="qSequenceInter" parameterClass="Map" resultClass="java.util.HashMap">  
	SELECT $SEQ_NAME$.NEXTVAL NEXTVAL 
	FROM   DUAL
  </select>
    
  <select id="qSysdatebyDual" resultClass="java.util.HashMap">  
	SELECT TO_CHAR(SYSDATE, 'yyyyMMddHH24miss') CUR_TIME
	FROM   DUAL
  </select>
  
</sqlMap>