<?xml version = "1.0" encoding="utf-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="daily_game">
    <select id="qryGameName" parameterClass="Map" resultClass="java.lang.String">
        SELECT trim(context_name)  as context_name
        FROM daily_game
        where context_code = #CONTEXT_CODE#
        and sp_code = #SP_CODE#
        and SYSDATE between to_date(eff_date, 'yyyymmdd') and
        to_date(exp_date, 'yyyymmdd')
    </select>
</sqlMap>