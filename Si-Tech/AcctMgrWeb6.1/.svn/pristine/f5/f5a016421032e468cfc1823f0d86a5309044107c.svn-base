public class S8248OrderInfoCompQryInDTO extends CommonInDTO {

	/**
	 * 
	 */
	private static final long serialVersionUID = -28117993289407398L;

	@ParamDesc(path = "BUSI_INFO.PHONE_NO", cons = ConsType.CT001, type = "String", len = "20", desc = "手机号码", memo = "略")
	private String phoneNo;

	@ParamDesc(path = "BUSI_INFO.START_DATE", cons = ConsType.CT001, type = "String", len = "20", desc = "开始年月", memo = "略")
	private String startDate;

	@ParamDesc(path = "BUSI_INFO.END_DATE", cons = ConsType.CT001, type = "String", len = "20", desc = "结束年月", memo = "略")
	private String endDate;

	@ParamDesc(path = "BUSI_INFO.REGION_ID", cons = ConsType.CT001, type = "String", len = "20", desc = "地市编码号", memo = "略")
	private String regionId;

	public String getPhoneNo() {
		return phoneNo;
	}

	public void setPhoneNo(String phoneNo) {
		this.phoneNo = phoneNo;
	}

	public String getStartDate() {
		return startDate;
	}

	public void setStartDate(String startDate) {
		this.startDate = startDate;
	}

	public String getEndDate() {
		return endDate;
	}

	public void setEndDate(String endDate) {
		this.endDate = endDate;
	}

	public String getRegionId() {
		return regionId;
	}

	public void setRegionId(String regionId) {
		this.regionId = regionId;
	}

	@Override
	public void decode(MBean arg0) {
		super.decode(arg0);

		if (StringUtils.isNotEmptyOrNull(arg0.getStr(getPathByProperName("phoneNo")))) {
			phoneNo = arg0.getStr(getPathByProperName("phoneNo"));
		}

		if (StringUtils.isNotEmptyOrNull(arg0.getStr(getPathByProperName("regionId")))) {
			regionId = arg0.getStr(getPathByProperName("regionId"));
		}

		if (StringUtils.isNotEmptyOrNull(arg0.getStr(getPathByProperName("startDate")))) {
			startDate = arg0.getStr(getPathByProperName("startDate"));
		}

		if (StringUtils.isNotEmptyOrNull(arg0.getStr(getPathByProperName("endDate")))) {
			endDate = arg0.getStr(getPathByProperName("endDate"));
		}
	}

