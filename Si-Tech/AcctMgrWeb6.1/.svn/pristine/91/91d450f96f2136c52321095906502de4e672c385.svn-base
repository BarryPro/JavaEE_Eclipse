   	    <isNotEmpty prepend="" property="CREDITCARDFEE"> ,CREDITCARDFEE </isNotEmpty>	    
     	<isNotEmpty prepend="" property="RESERVEFEE1"> ,RESERVEFEE1 </isNotEmpty>
     	<isNotEmpty prepend="" property="RESERVEFEE2"> ,RESERVEFEE2 </isNotEmpty>
