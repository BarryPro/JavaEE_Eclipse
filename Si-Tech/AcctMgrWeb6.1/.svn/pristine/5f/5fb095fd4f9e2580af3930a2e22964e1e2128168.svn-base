<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
        PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">
        
<sqlMap namespace="ur_usergoodsprcattr_info">
	
	<select id="qPrcId" parameterClass="Map" resultClass="java.util.HashMap">
  		SELECT PRC_ID FROM UR_USERGOODSPRCATTR_INFO
  		WHERE ATTR_ID = #ATTR_ID#
  		AND ID_NO = #ID_NO#
  		AND PRC_ID = #PRC_ID#
	</select>
	
</sqlMap>