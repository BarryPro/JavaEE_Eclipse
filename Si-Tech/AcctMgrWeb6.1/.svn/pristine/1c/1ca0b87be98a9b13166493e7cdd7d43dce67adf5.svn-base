<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="bal_paylimit_conf">

  <select id="qLimitMoney"  parameterClass="Map" resultClass="java.util.HashMap">
	select nvl(LIMIT_FEE, 0) FEE, GRADE_LEVEL from BAL_PAYLIMIT_CONF
	where OP_CODE = #OP_CODE#
	and OP_TYPE= #OP_TYPE#
	and ( REGION_CODE = #REGION_CODE# or REGION_CODE = 0 )
  </select>
  

</sqlMap>

