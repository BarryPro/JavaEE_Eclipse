<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="bal_recoveryplan_conf">

	<select id="qRecoverPlan" parameterClass="Map" resultClass="java.util.HashMap">
		SELECT ACCT_ITEM_CODE,PAY_TYPE
		FROM BAL_RECOVERYPLAN_CONF
		WHERE PAY_TYPE = #PAY_TYPE#
	</select>
	
</sqlMap>
