<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="in_btoamsgsend_info">

	<insert id="insert" parameterClass="Map">
	  INSERT INTO IN_BTOAMSGSEND_INFO (SEQ_NO, PHONE_NO, TEMPLATE_ID, SMS_NAME, CONTENT, 
	  			TOPIC_ID, INSERT_TIME, SEND_RESULT, RESEND, REMARK)
	  VALUES (#SEQ_NO#, #PHONE_NO#, #TEMPLATE_ID#, #SMS_NAME#, #CONTENT#,
	   #TOPIC_ID#, sysdate, #SEND_RESULT#, #RESEND#, #REMARK#)
	</insert>
	
</sqlMap>