package com.sitech.acctmgr.atom.domains.user;

/**
 * 群组成员（含家长）实体
 * Created by wangyla on 2016/8/18.
 */
public class UserGroupMbrEntity {
    private long idNo; //成员ID_NO
    private long groupId; //群组ID
    private long memberRoleId; //群组角色ID标识
    private String groupType; //群组类型
    private long groupIdNo; //群组对应的虚拟用户ID

    public long getIdNo() {
        return idNo;
    }

    public void setIdNo(long idNo) {
        this.idNo = idNo;
    }

    public long getGroupId() {
        return groupId;
    }

    public void setGroupId(long groupId) {
        this.groupId = groupId;
    }

    public long getMemberRoleId() {
        return memberRoleId;
    }

    public void setMemberRoleId(long memberRoleId) {
        this.memberRoleId = memberRoleId;
    }

    public String getGroupType() {
        return groupType;
    }

    public void setGroupType(String groupType) {
        this.groupType = groupType;
    }

    public long getGroupIdNo() {
        return groupIdNo;
    }

    public void setGroupIdNo(long groupIdNo) {
        this.groupIdNo = groupIdNo;
    }

    @Override
    public String toString() {
        return "UserGroupMbrEntity{" +
                "idNo=" + idNo +
                ", groupId=" + groupId +
                ", memberRoleId=" + memberRoleId +
                ", groupType='" + groupType + '\'' +
                ", groupIdNo=" + groupIdNo +
                '}';
    }
}
