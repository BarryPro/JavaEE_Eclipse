<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="cs_custbillqry_info">
    <select id="qryBillQryFlag" parameterClass="Map" resultClass="java.lang.String">
        SELECT QRY_FLAG
        FROM CS_CUSTBILLQRY_INFO
        WHERE BUSI_TYPE = '01'
        AND ID_NO = #ID_NO#
        AND SYSDATE >= EFF_DATE
    </select>
</sqlMap>