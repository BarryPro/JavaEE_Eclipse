<sqlMap namespace="cs_userdetail_info_chg">

  <insert id="insert" parameterClass="Map">
  	<!-- update by zhangleij 20170306 修改状态变更后同步账务和计费的小表模型
	insert into CS_USERDETAIL_INFO_$TAB_TAIL$
	  (SEQ_NO, OP_FLAG, ID_NO,
	   RUN_CODE, RUN_TIME, USER_GRADE_CODE,
	   STOP_FLAG, OWED_FLAG, USER_PASSWD,
	   PASSWD_TYPE, CARD_TYPE, VIP_CARD_NO, VIP_CREATE_TYPE,
	   OLD_RUN, QUERY_CDRFLAG, LMT_ADJUST_TYPE,
	   OP_TIME, LOGIN_NO, LOGIN_ACCEPT,
	   OP_CODE, OP_NOTE, SYSTEM_NOTE,
	   BAK1, BAK2)
      SELECT #SEQ_NO# SEQ_NO, #OP_FLAG# OP_FLAG, ID_NO,
	   RUN_CODE, RUN_TIME, USER_GRADE_CODE,
	   STOP_FLAG, OWED_FLAG, USER_PASSWD,
	   PASSWD_TYPE, CARD_TYPE, VIP_CARD_NO, VIP_CREATE_TYPE,
	   OLD_RUN, QUERY_CDRFLAG, LMT_ADJUST_TYPE,
	   OP_TIME, LOGIN_NO, LOGIN_ACCEPT,
	   OP_CODE, OP_NOTE, SYSTEM_NOTE,
	   BAK1, BAK2
      FROM CS_USERDETAIL_INFO WHERE ID_NO=#ID_NO# -->
      insert into CS_USERDETAIL_INFO_$TAB_TAIL$
	  (ID_NO,
	   RUN_CODE, RUN_TIME, USER_GRADE_CODE,
	   STOP_FLAG, OWED_FLAG, USER_PASSWD,
	   PASSWD_TYPE, CARD_TYPE, VIP_CARD_NO, VIP_CREATE_TYPE,
	   OLD_RUN, QUERY_CDRFLAG, LMT_ADJUST_TYPE,
	   OP_TIME, LOGIN_NO, LOGIN_ACCEPT,
	   OP_CODE, OP_NOTE, SYSTEM_NOTE,
	   BAK1, BAK2,
	   GET_TIME,GET_TYPE,GET_NO)
      SELECT ID_NO,
	   RUN_CODE, RUN_TIME, USER_GRADE_CODE,
	   STOP_FLAG, OWED_FLAG, USER_PASSWD,
	   PASSWD_TYPE, CARD_TYPE, VIP_CARD_NO, VIP_CREATE_TYPE,
	   OLD_RUN, QUERY_CDRFLAG, LMT_ADJUST_TYPE,
	   OP_TIME, LOGIN_NO, LOGIN_ACCEPT,
	   OP_CODE, OP_NOTE, SYSTEM_NOTE,
	   BAK1, BAK2,
	   SYSDATE, #GET_TYPE# GET_TYPE, #GET_NO# GET_NO
      FROM CS_USERDETAIL_INFO WHERE ID_NO=#ID_NO#
  </insert>
  
</sqlMap>


