	private String acctBookItemName = "";
	@JSONField(name="BALANCE")
	@ParamDesc(path="BALANCE",cons=ConsType.CT001,type="long",len="20",desc="预存",memo="略")
	private long balance = 0;
	@JSONField(name="ACCTBOOK_ITEM_TYPE")
	@ParamDesc(path="ACCTBOOK_ITEM_TYPE",cons=ConsType.CT001,type="String",len="50",desc="账目项类型",memo="0:专款, 1:非专款")
	private String acctBookItemType = "";
