<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="bal_checkmsg_info_his">

	<insert id="ins" parameterClass="Map">
		INSERT INTO
		BAL_CHECKMSG_INFO_HIS(BANK_CODE,CHECK_NO,BANK_COUNT,OWNER_UNIT,CHECK_MONEY,
		OWNER_NAME,CONTACT_PERSON,OWNER_ID,CHECK_STATUS,CONFIRM_STATUS,LOGIN_NO,OP_CODE,
		OP_TIME,REMARK,UPDATE_ACCEPT,UPDATE_TYPE,UPDATE_TIME,UPDATE_DATE,UPDATE_LOGIN,UPDATE_CODE)
		SELECT  BANK_CODE,CHECK_NO,BANK_COUNT,OWNER_UNIT,CHECK_MONEY,
		OWNER_NAME,CONTACT_PERSON,OWNER_ID,CHECK_STATUS,CONFIRM_STATUS,LOGIN_NO,OP_CODE,
		OP_TIME,REMARK,#UPDATE_ACCEPT#,#UPDATE_TYPE#,sysdate,#UPDATE_DATE#,#UPDATE_LOGIN#,#UPDATE_CODE#
		from BAL_CHECKMSG_INFO
		WHERE BANK_CODE=#BANK_CODE#
		AND CHECK_NO=#CHECK_NO#
	</insert>

</sqlMap>
