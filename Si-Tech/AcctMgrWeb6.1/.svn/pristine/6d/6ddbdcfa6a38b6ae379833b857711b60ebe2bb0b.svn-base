package com.sitech.acctmgr.atom.domains.query;

import com.alibaba.fastjson.annotation.JSONField;
import com.sitech.jcfx.anno.ConsType;
import com.sitech.jcfx.anno.ParamDesc;

public class TransLimitEntity {
	@JSONField(name = "OPER_PHONE")
	@ParamDesc(path="OPER_PHONE",cons=ConsType.CT001,type="String",len="15",desc="操作员手机号码",memo="")
	protected String operPhone;
	@JSONField(name = "MONTH_MONEY")
	@ParamDesc(path="MONTH_MONEY",cons=ConsType.CT001,type="long",len="15",desc="赠送金额",memo="单位：分")
	protected long monthMoney;
	@JSONField(name = "LIMIT_PAY")
	@ParamDesc(path="LIMIT_PAY",cons=ConsType.CT001,type="long",len="15",desc="金额上限",memo="单位：分")
	protected long limitPay;
	@JSONField(name = "TOTAL_PAYED")
	@ParamDesc(path="TOTAL_PAYED",cons=ConsType.CT001,type="long",len="15",desc="已赠送金额",memo="单位：分")
	protected long totalPayed;
	@JSONField(name = "REMAIN_PAY")
	@ParamDesc(path="REMAIN_PAY",cons=ConsType.CT001,type="long",len="15",desc="可赠送金额",memo="单位：分")
	protected long remainPay;
	
	@JSONField(name = "BEGIN_TIME")
	@ParamDesc(path="BEGIN_TIME",cons=ConsType.CT001,type="String",len="18",desc="开始时间",memo="")
	protected String beginTime;
	@JSONField(name = "END_TIME")
	@ParamDesc(path="END_TIME",cons=ConsType.CT001,type="String",len="18",desc="结束时间",memo="")
	protected String endTime;
	/**
	 * @return the operPhone
	 */
	public String getOperPhone() {
		return operPhone;
	}
	/**
	 * @param operPhone the operPhone to set
	 */
	public void setOperPhone(String operPhone) {
		this.operPhone = operPhone;
	}
	/**
	 * @return the monthMoney
	 */
	public long getMonthMoney() {
		return monthMoney;
	}
	/**
	 * @param monthMoney the monthMoney to set
	 */
	public void setMonthMoney(long monthMoney) {
		this.monthMoney = monthMoney;
	}
	/**
	 * @return the limitPay
	 */
	public long getLimitPay() {
		return limitPay;
	}
	/**
	 * @param limitPay the limitPay to set
	 */
	public void setLimitPay(long limitPay) {
		this.limitPay = limitPay;
	}
	/**
	 * @return the totalPayed
	 */
	public long getTotalPayed() {
		return totalPayed;
	}
	/**
	 * @param totalPayed the totalPayed to set
	 */
	public void setTotalPayed(long totalPayed) {
		this.totalPayed = totalPayed;
	}
	/**
	 * @return the remainPay
	 */
	public long getRemainPay() {
		return remainPay;
	}
	/**
	 * @param remainPay the remainPay to set
	 */
	public void setRemainPay(long remainPay) {
		this.remainPay = remainPay;
	}
	/**
	 * @return the beginTime
	 */
	public String getBeginTime() {
		return beginTime;
	}
	/**
	 * @param beginTime the beginTime to set
	 */
	public void setBeginTime(String beginTime) {
		this.beginTime = beginTime;
	}
	/**
	 * @return the endTime
	 */
	public String getEndTime() {
		return endTime;
	}
	/**
	 * @param endTime the endTime to set
	 */
	public void setEndTime(String endTime) {
		this.endTime = endTime;
	}


}
