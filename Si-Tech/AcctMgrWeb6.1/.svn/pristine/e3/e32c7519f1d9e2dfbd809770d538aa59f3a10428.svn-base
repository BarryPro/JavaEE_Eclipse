<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="cs_feeawokeadd_info">
	<select id="qAwokeFee" parameterClass="Map" resultClass="java.util.HashMap">
		SELECT	AWOKE_FEE FROM CS_FEEAWOKEADD_INFO
		WHERE REAWOKE_DAY <![CDATA[>=]]> #REAWOKE_DAY#
		AND VALID_FLAG = #VALID_FLAG#
		AND ID_NO=#ID_NO#
	</select>
</sqlMap>
