<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="remind_qinqing_control">	
	<select id="qry" parameterClass="Map" resultClass="java.lang.Long">
		select count(1) from remind_qinqing_control
		where msisdn=#PHONE_NO#
	</select>

	<insert id="insert" parameterClass="Map" >
		insert into remind_qinqing_control
		(msisdn,eff_date,exp_date)
		values(#PHONE_NO#,to_char(sysdate,'yyyymmddhh24miss'),'21000101000000')
	</insert>

	<delete id="delete" parameterClass="Map">
		delete from
		remind_qinqing_control
		where msisdn=#PHONE_NO#
	</delete>
	
	<insert id="insertHis" parameterClass="Map" >
		insert into remind_qinqing_control_his
		(msisdn,eff_date,exp_date,op_date)
		select 	msisdn,eff_date,exp_date,to_char(sysdate,'yyyymmddhh24miss')
		from 	REMIND_QINQING_CONTROL
		where 	msisdn=#PHONE_NO#
	</insert>

</sqlMap>