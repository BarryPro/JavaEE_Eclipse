<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap
        PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="cct_creditclass_conf">
	<select id="qry" parameterClass="Map" resultClass="java.lang.Long">
		SELECT nvl(limit_owe,0) 
		FROM cct_creditclass_conf
		WHERE credit_class = #CREDIT_CLASS#
	</select>
   
</sqlMap>