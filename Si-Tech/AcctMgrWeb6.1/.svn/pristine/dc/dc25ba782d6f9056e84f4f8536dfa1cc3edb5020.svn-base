		<property name="payManage" ref="payManageEnt" />
		<property name="writeOffer" ref="writeOfferEnt" />
		<property name="balance" ref="balanceEnt" />
		<property name="payOpener" ref="payOpenerEnt" />
		<property name="prod" ref="prodEnt" />
