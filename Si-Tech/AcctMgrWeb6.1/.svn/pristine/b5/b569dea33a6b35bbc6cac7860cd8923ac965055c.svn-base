<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">


<sqlMap namespace="srateflagcode">

	<select id="qFlag" parameterClass="Map" resultClass="java.lang.Integer">
		SELECT FLAG_CODE ATTR_VAL, FLAG_CODE_NAME FLAG_CODE_NAME
		FROM SRATEFLAGCODE
		WHERE REGION_CODE=#REGION_CODE#
		AND RATE_CODE=#RATE_CODE#
	</select>


</sqlMap>