<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap
        PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="ct_adcgrpcust_info">
    <select id="qCustContactPhone" parameterClass="Map" resultClass="java.lang.String">
        SELECT NVL(ECTEL,'NULL') ECTEL
        FROM CT_ADCGRPCUST_INFO
        WHERE CUST_ID = #CUST_ID#
    </select>

</sqlMap>