<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">


<sqlMap namespace="cBillStatCfg">

	<select id="qMonthCfg" parameterClass="Map" resultClass="java.lang.Integer">
		SELECT COUNT(*) CNT FROM CBILLSTATCFG
		WHERE REGION_CODE=#REGION_CODE#
		AND MODE_CODE=#MODE_CODE#
		AND DAY_FLAG=0
	</select>


</sqlMap>