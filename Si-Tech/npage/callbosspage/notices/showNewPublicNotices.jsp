ontextPath()%>/npage/notices/noticesAlertWindowMain.jsp?userId=<%=userId%>&showId='+showId;    var opts = 'width=' + 800 + ',height=' + 600 +',scroollbars=yes';    sysPopupWinOpen(url,opts);}$(document).ready(function () {    window.setInterval(function () {    	//showMessageDialog();    	queryMaxIdAjax();    }, 10000);});//rdShowMessageDialog("messs");</script>