<%@	page contentType="text/html;charset=GBK"%>
<%@ include file="/npage/include/public_title_name.jsp" %>
<%
	request.setCharacterEncoding("GBK");
%>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<title>合约计划购机</title>
<%	
    String opCode=request.getParameter("opCode");
		String opName=request.getParameter("opName");	
    String workNo=(String)session.getAttribute("workNo");
    String regionCode=(String)session.getAttribute("regCode");
	//	boolean workNoFlag=false;
	//	if(workNo.substring(0,1).equals("k"))
	//		workNoFlag=true;
%>
  </script>
	<META content=no-cache http-equiv=Pragma>
	<META content=no-cache http-equiv=Cache-Control>
	<META content="MSHTML 5.00.3315.2870" name=GENERATOR>
	<script language="javascript">
			/*************************初始化*****************************/
			$(function() {
				$('#opCode').val('<%=opCode%>');
				$('#opName').val('<%=opName%>');
				var opCode = '<%=opCode%>';
				// 设置选中的业务
				if(opCode == 'e505') {
				 $('#backaccept_id').css('display','block');
	  	   $('#opCode').val('e506');
	  	   $('#opName').val('合约计划购机冲正');
	  	   /*ningtn 网站承载合约计划*/
	  	   $("#bltys").css("display","none");
				}else if(opCode == 'e506') {
					$('input[name="opFlag"][value="two"]').attr("checked","checked");
					$('#backaccept_id').css('display','block');
		  	   $('#opCode').val('e506');
		  	   $('#opName').val('合约计划购机冲正');
		  	   /*ningtn 网站承载合约计划*/
		  	   $("#bltys").css("display","none");
				}
				
				//注册单选的事件
			    $('input[name="opFlag"]').click(function() {
			  		var value = $(this).val();
			  	    if(value != "one") {
			  	  	   $('#backaccept_id').css('display','block');
			  	  	   $('#opCode').val('e506');
			  	  	   $('#opName').val('合约计划购机冲正');
			  	  	   /*ningtn 网站承载合约计划*/
			  	  	   $("#bltys").css("display","none");
			  	    }else {
			  	  	   $('#backaccept_id').css('display','none');
			  	  	   $('#opCode').val('e505');
			  	  	   $('#opName').val('合约计划购机');
			  	  	   /*ningtn 网站承载合约计划*/
			  	  	   $('#bltys').css('display','block');
			  	    }
			    });
			    
				/*************************提交按钮注册事件*****************************/
				$('#submit_btn').click(function() {
						submit_click();
				})
				/*************************清除按钮注册事件*****************************/
				$('#reset_btn').click(function() {
						$('#backaccept').val('');
				})
			})
			
			/*************************提交按钮事件执行体*****************************/
			function submit_click() {
					var value = $('input[name="opFlag"][checked]').val();
					// 不同的值会调用不同的服务
					if( value == 'one' ) {
						//申请
						frm.action="se505.jsp";
					}else if( value == 'two' ) {
						//冲正
						frm.action="se506.jsp";
					}
					frm.submit();	
					return true;
			}
	</script>
</head>
<body>
<form name="frm" method="POST">
 	<input type="hidden" name="opCode" id="opCode" >
	<input type="hidden" name="opName" id="opName" >
	<%@ include file="/npage/include/header.jsp" %>
	<div class="title">
		<div id="title_zi">选择操作类型</div>
	</div>
<table cellspacing="0">
	<tr> 
		<td class="blue" width="20%">操作类型</td>
		<td colspan="3">
			
			<input type="radio" name="opFlag" value="two" checked>冲正
		</td>
	</tr>    
	<tr> 
		<td class="blue">手机号码 </td>
		<td> 
			<input type="text" size="12" name="activePhone" id="activePhone" value="<%=activePhone%>" v_minlength=1 v_maxlength=16 v_type="mobphone" v_must=1 maxlength="11" index="0" class="InputGrey" readOnly>
				<font color="orange">*</font>
		</td>
	</tr>
	<tr style="display:none" id="backaccept_id"> 
		<td class="blue">业务流水</td>
		<td colspan="3">
			<input class="button" type="text" name="backaccept" id="backaccept" v_must=1 >
				<font color="orange">*</font>
		</td>
	</tr>
	<!-- ningtn 渠道承载合约计划 start-->
	<tr id="bltys" style="display:block" > 
		<td class="blue">业务办理类型</td>
		<td >
			<select id="banlitype" name="banlitype" >
			<option value="0">前台办理</option>
			<option value="1">网站预约</option>
			</select>
		</td>
	</tr>
	<!-- ningtn 渠道承载合约计划 end-->
	<tr> 
		<td colspan="4" align="center" id="footer"> 
			<input class="b_foot" type=button name="confirm" id="submit_btn" value="确认" index="2">    
			<input class="b_foot" type=button name=back id="reset_btn" value="清除">
			<input class="b_foot" type=button name=qryP value="关闭" onClick="removeCurrentTab();">
		</td>
	</tr>
</table>
    <%@ include file="../../npage/common/pwd_comm.jsp" %>
    <%@ include file="/npage/include/footer_simple.jsp" %>
</form>
</body>
</html>
